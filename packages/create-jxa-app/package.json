  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "microbundle": "^0.13.0"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "fs-extra": "^9.1.0",
    "hyperquest": "^2.1.3",
    "kleur": "^4.1.4",
    "prompts": "^2.4.0",
    "sade": "^1.7.4",
    "semver": "^7.3.4",
