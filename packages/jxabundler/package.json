{
  "name": "jxabundler",
  "version": "1.0.0-alpha.0",
  "main": "dist/cli.js",
  "source": "src/index.js",
  "bin": "dist/cli.js",
  "license": "bsd-2-clause",
  "description": "MacOS Javascript JavaScript for Automation (JXA) bundler. Creates MacOS Apps, Commandline Scripts. Allows to use libaries from NPM.",
  "keywords": [
    "osx",
    "macos",
    "osascript",
    "jxa",
    "javascript"
  ],
  "repository": "aheissenberger/macos-jxa-bundler/jxabundler",
  "author": "<Andreas Heissenberger> andreas@heissenberger.at",
  "dependencies": {
    "@babel/core": "^7.12.17",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "asyncro": "^3.0.0",
    "camelcase": "^6.2.0",
    "kleur": "^4.1.4",
    "macos-version": "^5.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-terser": "^7.0.2",
    "sade": "^1.7.4",
    "tiny-glob": "^0.2.8"
  },
  "scripts": {
    "build": "microbundle --format cjs --target node src/cli.js",
    "dev": "microbundle watch --format cjs --target node src/cli.js"
  },
  "devDependencies": {
    "microbundle": "^0.13.0"
  }
}
