{
  "name": "jxabundler",
  "version": "1.0.16",
  "main": "dist/cli.cjs",
  "source": "src/index.js",
  "bin": "dist/cli.cjs",
  "license": "bsd-2-clause",
  "description": "MacOS Javascript for Automation (JXA) bundler. Creates MacOS Apps, Commandline Scripts. Allows to use libaries from NPM.",
  "keywords": [
    "osx",
    "macos",
    "osascript",
    "jxa",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aheissenberger/macos-jxa-bundler.git",
    "directory": "packages/jxabundler"
  },
  "author": "Andreas Heissenberger <andreas@heissenberger.at>",
  "homepage": "https://github.com/aheissenberger/macos-jxa-bundler/jxabundler",
  "bugs": {
    "url": "https://github.com/aheissenberger/macos-jxa-bundler/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/core": "^7.12.17",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "asyncro": "^3.0.0",
    "camelcase": "^6.2.0",
    "kleur": "^4.1.4",
    "macos-version": "^6.0.0",
    "rollup": "^2.60.2",
    "rollup-plugin-terser": "^7.0.2",
    "sade": "^1.7.4",
    "tiny-glob": "^0.2.9",
    "tslib": "^2.3.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "microbundle --format cjs -i src/cli.js -o dist/cli.cjs --target node",
    "dev": "microbundle watch --format cjs --target node src/cli.js",
    "prepublishOnly": "microbundle -i src/cli.js -o dist/cli.cjs -f cjs --target node --compress --no-sourcemap"
  },
  "devDependencies": {
    "macos-version": "^6.0.0",
    "microbundle": "^0.14.2"
  },
  "gitHead": "eaa0d14f7cd765c826277f2255bdf08609ada059"
}
